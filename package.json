  "devDependencies": {
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0",
    "open": "^8.4.0",
    "supertest": "^6.2.3"
  },
  "dependencies": {
    "avalanche": "^3.14.0",
    "axios": "^0.27.2",
