{
  "name": "avax-eth-test",
  "type": "module",
  "version": "1.0.0",
  "description": "NodeJS test for custom blockchain in Avax",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "test": "mocha **/*.test.js && mocha --reporter json > reports/test-results.json",
